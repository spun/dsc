{
  "name": "dsc",
  "version": "2.0.0-alpha1",
  "description": "Bookmarklet for downloading media from supported pages",
  "scripts": {
    "dev": "rollup --config rollup.config.dev.js --watch",
    "build": "rollup --config rollup.config.prod.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest __tests__/unitTests -c jest-unit.config.js --verbose",
    "test:integration": "server-test _runHttp-server 8080 _runJestIntegration",
    "_runHttp-server": "node ./utils/tests_server.js",
    "_runJestIntegration": "jest __tests__/integrationTests -c jest-integration.config.js --verbose"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.2",
    "@types/expect-puppeteer": "^5.0.1",
    "@types/jest": "^28.1.6",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/node": "^18.6.1",
    "@types/puppeteer": "^5.4.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "jest-puppeteer": "^6.1.1",
    "postcss": "^8.4.14",
    "puppeteer": "^11.0.0",
    "rollup": "^2.77.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
