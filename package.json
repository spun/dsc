{
  "name": "dsc",
  "version": "2.0.0-alpha1",
  "description": "Bookmarklet for downloading media from supported pages",
  "scripts": {
    "dev": "rollup --config rollup.config.dev.js --watch",
    "build": "rollup --config rollup.config.prod.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest __tests__/unitTests -c jest-unit.config.js --verbose",
    "test:integration": "server-test _runHttp-server 8080 _runJestIntegration",
    "_runHttp-server": "node ./utils/tests_server.js",
    "_runJestIntegration": "jest __tests__/integrationTests -c jest-integration.config.js --verbose"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.1",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest": "^26.0.23",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/node": "^15.12.4",
    "@types/puppeteer": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^27.0.4",
    "jest-puppeteer": "^5.0.4",
    "postcss": "^8.3.5",
    "puppeteer": "^10.0.0",
    "rollup": "^2.52.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "start-server-and-test": "^1.12.5",
    "ts-jest": "^27.0.3",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  }
}
