{
  "name": "dsc",
  "version": "2.0.0-alpha1",
  "description": "Bookmarklet for downloading media from supported pages",
  "scripts": {
    "dev": "rollup --config rollup.config.dev.js --watch",
    "build": "rollup --config rollup.config.prod.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest __tests__/unitTests -c jest-unit.config.js --verbose",
    "test:integration": "server-test _runHttp-server 8080 _runJestIntegration",
    "_runHttp-server": "node ./utils/tests_server.js",
    "_runJestIntegration": "jest __tests__/integrationTests -c jest-integration.config.js --verbose"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.1",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.4.0",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/node": "^17.0.8",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.7",
    "jest-puppeteer": "^6.0.3",
    "postcss": "^8.4.5",
    "puppeteer": "^11.0.0",
    "rollup": "^2.63.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
