    "@types/node": "^14.14.20",
    "@types/puppeteer": "^5.4.2",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "postcss": "^8.2.3",
